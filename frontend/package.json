{
  "name": "allotment-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "dev": "vite -c config/vite.config.ts",
    "build": "tsc -b && vite build -c config/vite.config.ts",
    "build:analyze": "npm run build && node scripts/analyze-bundle.js",
    "lint": "npx @biomejs/biome lint --write ./src",
    "lint:unsafe": "npx @biomejs/biome lint --write --unsafe ./src",
    "format": "npx @biomejs/biome format --write ./src",
    "ci:check": "npx @biomejs/biome lint ./src && npx @biomejs/biome format ./src",
    "preview": "vite preview -c config/vite.config.ts",
    "test": "vitest run --pool=threads --config config/vitest.config.ts",
    "test:coverage": "vitest run --pool=threads --coverage --config config/vitest.config.ts",
    "test:synchronous": "vitest run --config config/vitest.config.ts",
    "profile": "node scripts/profile-auth.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^4.1.3",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@tanstack/react-query": "^5.69.0",
    "axios": "^1.8.4",
    "cmdk": "^1.1.1",
    "countries-list": "^3.1.1",
    "idb": "^8.0.2",
    "lucide-react": "^0.483.0",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-router-dom": "^7.4.0",
    "react-window": "^1.8.11",
    "sonner": "^2.0.2",
    "workbox-expiration": "^7.3.0",
    "workbox-precaching": "^7.3.0",
    "workbox-routing": "^7.3.0",
    "workbox-strategies": "^7.3.0",
    "workbox-window": "^7.3.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@tailwindcss/vite": "^4.0.15",
    "@tanstack/react-query-devtools": "^5.91.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.13.11",
    "@types/react-dom": "^19.0.0",
    "@types/react-window": "^1.8.8",
    "@vitejs/plugin-legacy": "^6.1.0",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "@vitest/coverage-v8": "^3.1.1",
    "@vitest/ui": "^3.2.4",
    "@welldone-software/why-did-you-render": "^10.0.1",
    "baseline-browser-mapping": "^2.8.31",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "globals": "^15.15.0",
    "jsdom": "^26.0.0",
    "lighthouse": "^12.8.2",
    "msw": "^2.7.3",
    "puppeteer": "^24.31.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.15",
    "tw-animate-css": "^1.2.4",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.3.2",
    "vite-imagetools": "^7.0.5",
    "vite-plugin-compression": "^0.3.1",
    "vite-plugin-pwa": "^0.21.2",
    "vitest": "^3.1.1"
  },
  "overrides": {
    "glob": "^9.0.0",
    "inflight": "npm:lru-cache@latest",
    "sourcemap-codec": "npm:@jridgewell/sourcemap-codec@latest"
  }
}
